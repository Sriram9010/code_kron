5	4	3	2	1	
5	4	3	2	1	
5	4	3	2	1	
5	4	3	2	1
#include <stdio.h>
int main() {
   int i,j;
   for(i=1;i<=4;i++){
       for(j=5;j>=1;j--){
           printf("%d\t",j);
        
       }
       printf("\n");
   }
    return 0;
}
=============================================================================================================================================
1 1 2 1 3 1
1 2 2 2 3 2
1 3 2 3 3 3
1 4 2 4 3 4
1 5 2 5 3 5
#include <stdio.h>
int main() {
    int i, n = 5;
    for(i = 1; i <= n; i++) {
        printf("1 %d 2 %d 3 %d\n", i, i, i);
    }
    return 0;
}
============================================================================================================================================
1	2	3	4	5	
2	4	6	8	10	
3	6	9	12	15	
4	8	12	16	20	
5	10	15	20	25
#include <stdio.h>
int main() {
    int i, j;
    for(i = 1; i <= 5; i++) {
        for(j = 1; j <= 5; j++) {
            printf("%d\t", i * j);
        }
        printf("\n");
    }
    return 0;
}
